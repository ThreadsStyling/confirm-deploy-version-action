{
  "name": "confirm-deploy-version-action",
  "version": "0.0.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/threadsstyling/confirm-deploy-version-action.git"
  },
  "private": true,
  "keywords": [
    "actions",
    "node",
    "setup"
  ],
  "license": "MIT",
  "description": "This action will confirm if a deployed version of a service/webapp matches a given commit",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "format": "prettier --write **/*.ts",
    "format-check": "prettier --check **/*.ts",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "yarn lint --fix",
    "package": "ncc build --source-map --license licenses.txt",
    "all": "yarn build && yarn format && yarn lint && yarn package"
  },
  "dependencies": {
    "@actions/core": "^1.10.1",
    "@actions/github": "^5.1.1"
  },
  "devDependencies": {
    "@types/node": "^16.18.59",
    "@types/node-fetch": "^2.5.11",
    "@typescript-eslint/eslint-plugin": "^6.9.0",
    "@typescript-eslint/parser": "^6.11.0",
    "@vercel/ncc": "^0.38.1",
    "eslint": "^8.52.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-github": "^4.10.1",
    "js-yaml": "^4.1.0",
    "prettier": "^3.0.3",
    "typescript": "^5.2.2"
  }
}
